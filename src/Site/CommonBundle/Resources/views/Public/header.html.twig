<!DOCTYPE html>
<html class="no-js" lang="zh-CN">
  <head>
    <title>{{ common.title }}</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    {% block stylesheets %}
      {% stylesheets filter='css_url_rewrite'
        '@SiteCommonBundle/Resources/public/libs/mediaelement/mediaelementplayer.css'
        '@SiteCommonBundle/Resources/public/libs/fancybox/source/jquery.fancybox.css'
        '@SiteCommonBundle/Resources/public/libs/fancybox/source/helpers/jquery.fancybox-buttons.css'
        '@SiteCommonBundle/Resources/public/libs/fancybox/source/helpers/jquery.fancybox-thumbs.css'
        '@SiteCommonBundle/Resources/public/libs/metro/css/m-styles.min.css'
        '@SiteCommonBundle/Resources/public/libs/jcontent/css/jcontent.css'
        '@SiteCommonBundle/Resources/public/css/*.css'
      %}
        <link rel="stylesheet" type="text/css" media="screen" href="{{ asset_url }}" />
      {% endstylesheets %}
      <!--[if IE 7]>
        {% stylesheets filter='css_url_rewrite'
          '@SiteCommonBundle/Resources/public/css/font-awesome-ie7.css'
        %}
          <link rel="stylesheet" type="text/css" media="screen" href="{{ asset_url }}" />
        {% endstylesheets %}
      <![endif]-->
    {% endblock %}
    <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
    <script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
    {% javascripts
      '@SiteCommonBundle/Resources/public/libs/jquery/jquery-2.0.0.js'
      '@SiteCommonBundle/Resources/public/libs/mediaelement/mediaelement-and-player.js'
      '@SiteCommonBundle/Resources/public/libs/jcontent/js/jquery.easing.min.1.3.js'
      '@SiteCommonBundle/Resources/public/libs/jcontent/js/jquery.jcontent.0.8.min.js'
      '@SiteCommonBundle/Resources/public/libs/bootstrap/js/bootstrap.js'
      '@SiteCommonBundle/Resources/public/libs/fancybox/lib/jquery.mousewheel-3.0.6.pack.js'
      '@SiteCommonBundle/Resources/public/libs/fancybox/source/jquery.fancybox.pack.js'
      '@SiteCommonBundle/Resources/public/libs/fancybox/source/helpers/jquery.fancybox-buttons.js'
      '@SiteCommonBundle/Resources/public/libs/fancybox/source/helpers/jquery.fancybox-media.js'
      '@SiteCommonBundle/Resources/public/libs/fancybox/source/helpers/jquery.fancybox-thumbs.js'
      '@SiteCommonBundle/Resources/public/libs/history/jquery.history.js'
      '@SiteCommonBundle/Resources/public/libs/custom/jquery.gkstate.js'
      '@SiteCommonBundle/Resources/public/libs/jquery-store/jquery.session.js'
      '@SiteCommonBundle/Resources/public/libs/jquery-store/jquery.sessionStorage.js'
      '@SiteCommonBundle/Resources/public/libs/jquery-form/jquery.form.js'
      '@SiteCommonBundle/Resources/public/libs/jquery-validation/dist/jquery.validate.js'
      '@SiteCommonBundle/Resources/public/libs/jquery-validation/dist/additional-methods.js'
      '@SiteCommonBundle/Resources/public/libs/jquery-validation/localization/messages_zh.js'
      '@SiteCommonBundle/Resources/public/libs/jquery-smooth-scroll/jquery.smooth-scroll.js'
    %}
      <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
  </head>
  <body>
    <!-- 加载顶部导航栏 -->
    {% include 'SiteCommonBundle:Public:navbar.html.twig' %}
    <!-- 全局布局层 -->
    <div class="wrap">
    <!-- 加载顶部导航 -->
      {% include 'SiteCommonBundle:Public:topnav.html.twig' %}
